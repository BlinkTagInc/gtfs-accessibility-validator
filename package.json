{
  "name": "gtfs-accessibility-validator",
  "version": "0.3.3",
  "description": "Test GTFS for accessibility information",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "accessibility"
  ],
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-accessibility-validator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blinktaginc/gtfs-accessibility-validator.git"
  },
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "gtfs-accessibility-validator": "dist/bin/gtfs-accessibility-validator.js"
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsup"
  },
  "dependencies": {
    "cli-table": "^0.3.11",
    "color-contrast-checker": "^2.1.0",
    "gtfs": "^4.18.1",
    "lodash-es": "^4.17.21",
    "pretty-error": "^4.0.0",
    "yargs": "^18.0.0",
    "yoctocolors": "^2.1.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24",
    "@types/yargs": "^17.0.35",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 20.11.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    },
    "hooks": {
      "after:bump": "npm run build"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": "prettier --write",
    "*.ts": "prettier --write",
    "*.json": "prettier --write"
  }
}
